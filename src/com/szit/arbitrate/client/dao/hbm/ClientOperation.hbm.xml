<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping package="com.szit.arbitrate.client.entity">
	<class name="ClientOperation" table="AG_CLIENT_OPERATION">
		<id name="id" column="ID" type="string" length="50">
			<generator class="uuid2"/>
		</id>
		<property name="operationName" column="OPERATION_NAME"/>
		<property name="operationInfo" column="OPERATION_INFO"/>
	</class>
</hibernate-mapping>