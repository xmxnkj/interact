<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.szit.arbitrate.client.entity">
	<class name="ClientNotOnlineNotify" table="cl_Client_NotOnline_Notify">
		<id name="id" column="ID" type="string" length="50">
            <generator class="uuid2"/>
        </id> 
        <property name="clientId" column="CLIENT_ID" />
        <property name="clientName" column="CLIENT_NAME" />
        <property name="gatherTime" column="GATHER_TIME" />
        <property name="caseId" column="CASE_ID" />
        <property name="caseExplain" column="CASE_EXPLAIN" />
        <property name="mediatorClientId" column="MEDIATOR_CLIENT_ID" />
	</class>
</hibernate-mapping>