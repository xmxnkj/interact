<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!--运动API日志-->
	<package name="arbitrate-api-log" namespace="/admin/api/log" extends="per-default">
		<!--API日志列表-->
		<action name="list" class="apiRecordLogAction" method="list">
			<result>/WEB-INF/jsp/admin/api/log/list.jsp</result>
		</action>
		<action name="showEditView" class="apiRecordLogAction" method="dispatcherEditPage">
			<result>/WEB-INF/jsp/admin/api/log/detailsform.jsp</result>
	    </action>		
	</package>
 </struts>