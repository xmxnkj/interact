<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.szit.arbitrate.mediation.entity">
	<class name="BasicData" table="ME_BASIC_DATA">
		<id name="id" column="ID" type="string" length="50">
            <generator class="uuid2"/>
        </id>
        <property name="dataType" column="DATA_TYPE" />
        <property name="dataTypeDesc" column="DATA_TYPE_DESC" />
        <property name="parentType" column="PARENT_TYPE" />
        <property name="dataValue" column="DATA_VALUE" />
	</class>
</hibernate-mapping>