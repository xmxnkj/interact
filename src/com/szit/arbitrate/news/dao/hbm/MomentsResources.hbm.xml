<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
	
	<hibernate-mapping package="com.szit.arbitrate.news.entity">
	<class name="MomentsResources" table="NEWS_MOMENTS_RESOURCES">
		<id name="id" column="ID" type="string" length="50">
			<generator class="uuid2"/>
		</id> 
		
		<property name="url" column="URL" type="string"/>

        <property name="rescreatedatatime" column="PLOAD_DATE" type="java.util.Date"/>
        
        <property name="link" column="LINK" type="string"/>
        
        <property name="isLunBo" column="isLunBo" type="string"/>
        
        <property name="title" column="title" type="string"/>
        
	</class>
</hibernate-mapping>